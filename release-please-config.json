{
	"plugins": [],
	"release-type": "node",
    "prerelease-type": "alpha",
    "release-as": "0.1.0",
    "prerelease": true,
	"packages": {
		".": {
			"release-type": "node",
			"include-component-in-tag": false,
			"group-pull-request-title-pattern": "chore: release-${version}",
			"pull-request-header": ":robot: Release candidate PR :rocket:",
			"changelog-path": "CHANGELOG.md",
			"options": {}
		}
	}
}
